            },
            {
              "name": "Medium",
              "size": "20px",
              "slug": "medium"
            },
            {
              "name": "Large",
              "size": "30px",
              "slug": "large"
            },
            {
              "name": "xLarge",
              "size": "40px",
              "slug": "x-large"
            }
          ]
        }
      }
    },
    "color": {
      "defaultDuotone": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "palette": [
        {
          "color": "#81ADC8",
          "name": "Pewter Blue",
          "slug": "pewter-blue"
        },
        {
          "color": "#CD4631",
          "name": "Vermillion",
          "slug": "vermillion"
        },
        {
          "color": "#EDF3F7",
          "name": "Alice Blue",
          "slug": "alice-blue"
        },
        {
          "color": "#F7EDE8",
          "name": "Champagne",
          "slug": "champagne"
        },

        {
          "color": "#4D4D4D",
          "name": "Davys Grey",
          "slug": "davys-grey"
        },
        {
          "color": "#000000",
          "name": "Black",
          "slug": "black"
        },
        {
          "color": "#FFFFFF",
          "name": "White",
          "slug": "white"
        },
        {
          "color": "transparent",
          "name": "Transparent",
          "slug": "transparent"
        }
      ]
    },
    "custom": {
      "fontWeight": {
        "medium": 500,
        "bold": 700
      },
      "lineHeight": {
        "100": "100%",
        "120": "120%",
        "140": "140%",
        "160": "160%"
      },
      "spacing": {
        "gap": "30px"
      }
    },
    "layout": {
      "contentSize": "1440px",
      "wideSize": "1440px"
    },
    "shadow": {
      "defaultPresets": false,
      "presets": [
        {
          "name": "Light",
          "slug": "light",
          "shadow": "0 0 50px rgb(0 0 0 / 10%);"
        }
      ]
    },
    "spacing": {
      "spacingSizes": [
        {
          "name": "xSmall",
          "size": "20px",
          "slug": "x-small"
        },
        {
          "name": "Small",
          "size": "clamp(30px, 4vw, 40px)",
          "slug": "small"
        },
        {
          "name": "Medium",
          "size": "clamp(40px, 6vw, 60px)",
          "slug": "medium"
        },
        {
          "name": "Large",
          "size": "clamp(50px, 8vw, 80px)",
          "slug": "large"
        },
        {
          "name": "xLarge",
          "size": "clamp(60px, 10vw, 100px)",
          "slug": "x-large"
        }
      ],
      "units": ["%", "px", "em", "rem", "vh", "vw"]
    },
    "typography": {
      "dropCap": false,
      "fluid": true,
      "fontFamilies": [
        {
          "fontFamily": "Montserrat Alternates, sans-serif",
          "name": "Montserrat Alternates",
          "slug": "montserrat-alternates",
          "fontFace": [
            {
              "fontDisplay": "block",
              "fontFamily": "Montserrat Alternates",
              "fontStretch": "normal",
              "fontStyle": "medium",
              "fontWeight": "500",
              "src": ["file:./assets/fonts/MontserratAlternates-Medium.woff2"]
            },
            {
              "fontDisplay": "block",
              "fontFamily": "Montserrat Alternates",
              "fontStretch": "normal",
              "fontStyle": "bold",
              "fontWeight": "700",
              "src": ["file:./assets/fonts/MontserratAlternates-Bold.woff2"]
            }
          ]
        },
        {
          "fontFamily": "monospace",
          "name": "Monospace",
          "slug": "monospace"
        }
      ],
      "fontSizes": [
        {
          "fluid": false,
          "name": "Small",
          "size": "12px",
          "slug": "small"
